<?xml version="1.0" encoding="UTF-8"?>
<web-app 
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
	<session-config>
    <session-timeout>5</session-timeout> <!-- Timeout in minutes -->
	</session-config>
    <jsp-config>
        <taglib>
            <taglib-uri>http://java.sun.com/jsp/jstl/fmt</taglib-uri>
            <taglib-location>/WEB-INF/lib/fmt-1_0.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
            <taglib-location>/WEB-INF/lib/c.tdl</taglib-location>
        </taglib>
    </jsp-config>
  
  <servlet>
	<servlet-name>abc</servlet-name>
	<servlet-class>com.telsuko.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>abc</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>xyz</servlet-name>
	<servlet-class>com.telsuko.SqServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>xyz</servlet-name>
  	<url-pattern>/sq</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>testServlet</servlet-name>
	<servlet-class>com.telsuko.testServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>testServlet</servlet-name>
  	<url-pattern>/testServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>signupServlet</servlet-name>
	<servlet-class>com.telsuko.signupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>signupServlet</servlet-name>
  	<url-pattern>/signupServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>Common</servlet-name>
	<servlet-class>com.telsuko.Common</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Common</servlet-name>
  	<url-pattern>/Common</url-pattern>
  </servlet-mapping>
  
</web-app>